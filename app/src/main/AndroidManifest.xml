<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.work.buitems_event_guide">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        android:windowSoftInputMode="stateHidden">
        <activity android:name=".SplashActivity"
            android:theme="@style/AppThemeComp">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/map.api" />

        <activity android:name=".MainActivity">

        </activity>
        <activity android:name=".Admin_login"
            android:theme="@style/AppThemeComp"/>
        <activity android:name=".Event_Info" />
        <activity
            android:name=".EditEventActivity"
            android:theme="@style/AppThemeComp" />
        <activity android:name=".Event_Edit" />
        <activity
            android:name=".mapActivity"
            android:label="Map Guide" />
    </application>

</manifest>